<Window x:Class="AlarmTool_eNodeB_Ericsson.FilterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AlarmTool_eNodeB_Ericsson"
        mc:Ignorable="d"
        Title="FilterWindow" Height="560" Width="300">
    <DataGrid x:Name="fGrid" AutoGenerateColumns="False">
        <DataGrid.Columns>
            <DataGridCheckBoxColumn Header="State" Binding="{Binding State}" />
            <DataGridTextColumn Header="AlarmName" Binding="{Binding AlarmName}" />
        </DataGrid.Columns>
    </DataGrid>
</Window>
